/**
 * Adds Sections for block layouts
 * SectionComponent and Blocks removed to prevent duplicated items from being added to custom block library
 * Basic Content etc will be added when block is in layout
 * Blocks from custom block library can be added in the future using their uuid, only block type instances have duplication issue, look into deepcloningtrait from section library
 */
function addSectionTemplates($templates) {
  //Create array of content blocks using label as index and value as uuid
  $block_content_blocks = BlockContent::loadMultiple(NULL);
  $content_blocks_array = [];
  foreach ($block_content_blocks as $contentblock) {
    $content_blocks_array[$contentblock->label()] = $contentblock->uuid();
  }
  
  
  foreach ($templates as $sectlibtemplate) {
    $sections_for_data = [];
    foreach ($sectlibtemplate['sections'] as $template) {
      //Look for existing, if there are any with same label, skip
      $lib_templates = \Drupal::entityTypeManager()->getStorage('section_library_template')->loadByProperties(['label' => $template['label']]);
      if (count($lib_templates) == 0) {
        //Removed Create SectionComponents and their respective BlockComponents to prevent duplicated blocks in custom block library
        $components = [];
        
        foreach ($template['blocks'] as $component_array) {
          //Block Content block, lookup if the block is in the block library and re-add uuid ex 'id'=>'block_content:ffffffff-ffff-ffff-ffff-ffffffffffff'
          if ($component_array['configuration']['id'] == 'block_content') {
            $component_array['configuration']['id'] = "block_content:" . $content_blocks_array[$component_array['configuration']['label']];
          }
          
          //Inline Blocks, base64 encoded to prevent exportvar from messing with it, convert back to string for deserialization
          if (array_key_exists('block_serialized', $component_array['configuration'])) {
            $component_array['configuration']['block_serialized'] = base64_decode($component_array['configuration']['block_serialized']);
          }
          
          $components[] = SectionComponent::fromArray($component_array);
        }
        
        $section = new Section($template['section_id'], $template['section'], $components, []);
        $section->setLayoutSettings($template['section']);

        $sections_for_data[] = $section;
      }
    }
    if (count($lib_templates) == 0) {
      //Load images and set them to be used as thumbnails
      $file_loc = \Drupal::service('file_system')->realpath(drupal_get_path('module', 'section_lib_importer')) . '/resources/' . $sectlibtemplate['filename'];
      $file_data = file_get_contents($file_loc);
      $file_saved = file_save_data($file_data, "public://section_library/" . $sectlibtemplate['filename'], FileSystemInterface::EXISTS_REPLACE);

      $data = [
        'type' => $sectlibtemplate['type'],
        'entity_type' => 'node',
        'label' => $sectlibtemplate['label'],
        'image' => [
          'target_id' => $file_saved->id(),
          'display' => 1
        ],
        'layout_section' => $sections_for_data
      ];
      //Create library template from section and image
      $template_ent = Drupal::entityTypeManager()->getStorage('section_library_template')->create($data);
      $template_ent->setOwnerId(1);
      $template_ent->save();
    }
  }
