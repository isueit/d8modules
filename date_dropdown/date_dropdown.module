<?php
/**
 * Implements hook_form_alter().
 */
function date_dropdown_form_alter(&$form, &$form_state, $form_id) {
  // Check if the form ID is the "node-county-job-opening-form".
  if ($form_id === 'node_county_job_opening_form') {

    // Check if the application deadline date field exists.
    if (isset($form['field_application_deadline_date']['widget'][0]['value']['year'])) {

      // Get the current year.
      $current_year = date('Y');

      // Define the range of years (current year to 2040).
      $years = range($current_year, 2040);

      // Set the options for the year dropdown.
      $form['field_application_deadline_date']['widget'][0]['value']['year']['#options'] = array_combine($years, $years);
    }
  }
}
